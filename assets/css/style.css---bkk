.grid {
position: relative;
/*background: #fff;*/
;
}
/* Common style */
.grid div {
position: relative;
cursor: pointer;
-webkit-transition: opacity 0.35s;
transition: opacity 0.35s;
}
.grid div:hover {
opacity: .8;
}
.grid div figcaption {
padding: 2em;
color: #fff;
text-transform: uppercase;
font-size: 1.25em;
-webkit-backface-visibility: hidden;
backface-visibility: hidden;
}
.grid div figcaption::before,
.grid div figcaption::after {
pointer-events: none;
}
.grid div figcaption,
.grid div figcaption > a {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}
/* Anchor will cover the whole item by default */
/* For some effects it will show as a button */
.grid div figcaption > a {
z-index: 1000;
text-indent: 200%;
white-space: nowrap;
font-size: 0;
opacity: 0;
}
/*---------------*/
/***** Marley *****/
/*---------------*/
div.effect-marley figcaption {
text-align: right;
}
div.effect-marley p {
text-align: center;
padding: 10px 0;
color: #fff;
}
div.effect-marley p {
bottom: 30px;
line-height: 1.5;
-webkit-transform: translate3d(0, 100%, 0);
transform: translate3d(0, 100%, 0);
}
div.effect-marley h2 {
top: 50px;
margin: 11% 0 10px 0;
-webkit-transition: -webkit-transform 0.35s;
transition: transform 0.35s;
-webkit-transform: translate3d(0, 100%, 0);
transform: translate3d(0, 100%, 0);
font-size: 24px;
text-align: center;
}
div.effect-marley:hover h2 {
-webkit-transform: translate3d(0, 0, 0);
transform: translate3d(0, 0, 0);
}
div.effect-marley p::after {
position: absolute;
top: 100%;
left: 0;
width: 100%;
height: 1px;
background: #fff;
content: '';
-webkit-transform: translate3d(0, 80px, 0);
transform: translate3d(0, 80px, 0);
}
div.effect-marley p::before {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 1px;
background: #fff;
content: '';
-webkit-transform: translate3d(0, 40px, 0);
transform: translate3d(0, 40px, 0);
}
div.effect-marley p::after,
div.effect-marley p::before,
div.effect-marley p {
opacity: 0;
-webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
transition: opacity 0.35s, transform 0.35s;
}
div.effect-marley:hover p::after,
div.effect-marley p::before,
div.effect-marley:hover p {
opacity: 1;
-webkit-transform: translate3d(0, 0, 0);
transform: translate3d(0, 0, 0);
}
